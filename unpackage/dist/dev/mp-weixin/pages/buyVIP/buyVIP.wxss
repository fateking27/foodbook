  top: -24rpx;
  left: -2rpx;
  line-height: 48rpx;
  background: url(http://localhost:4000/public/images/tuijian@3x.png) no-repeat center center/98rpx 48rpx;
}
.recommend-box .recommend-type .recommend-month {
  font-size: 36rpx;
  margin-bottom: 26rpx;
  margin-top: 48rpx;
}
.recommend-box .recommend-type .recommend-price {
  font-size: 44rpx;
  font-weight: 500;
  color: #deb062;
  margin-bottom: 26rpx;
}
.recommend-box .recommend-type .recommend-oldPrice {
  font-size: 28rpx;
  color: #a49fa0;
