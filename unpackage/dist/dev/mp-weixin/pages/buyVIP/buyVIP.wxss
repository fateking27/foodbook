.recommend-box {
  display: flex;
  justify-content: space-around;
}
.recommend-box .recommend-type {
  position: relative;
  width: 210rpx;
  height: 232rpx;
  border-radius: 16rpx;
  border: 1px solid #e0e0e0;
  background-color: #fbfcfe;
  text-align: center;
  line-height: 1;
  box-sizing: border-box;
}
.recommend-box .recommend-type.active {
  border: 1px solid #dcb66a;
  background-color: #fbf7ef;
}
.recommend-box .recommend-type:first-child::after {
  content: '推荐';
  display: block;
  width: 98rpx;
  height: 48rpx;
  color: white;
  font-size: 28rpx;
  border-radius: 14rpx;
